{
    "name": "vibe-client",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint",
        "test": "jest"
    },
    "dependencies": {
        "babel-jest": "^24.9.0",
        "buefy": "^0.8.8",
        "bulma": "^0.8.0",
        "chart.js": "^2.9.3",
        "core-js": "^3.3.6",
        "d3": "^5.14.2",
        "jest-css-modules-transform": "^3.1.0",
        "material-design-icons-iconfont": "^5.0.1",
        "node-fetch": "^2.6.0",
        "sinon": "^7.5.0",
        "vue": "^2.6.10",
        "vue-chartjs": "^3.5.0",
        "vue-router": "^3.1.3",
        "vuex": "^3.0.1"
    },
    "devDependencies": {
        "@mdi/font": "^4.5.95",
        "@vue/cli-plugin-babel": "^4.0.0",
        "@vue/cli-plugin-eslint": "^4.0.0",
        "@vue/cli-service": "^5.0.8",
        "@vue/test-utils": "^1.0.0-beta.29",
        "babel-core": "^7.0.0-bridge.0",
        "babel-eslint": "^10.0.3",
        "cypress": "^3.7.0",
        "deepmerge": "^4.2.2",
        "eslint": "^5.16.0",
        "eslint-plugin-vue": "^5.0.0",
        "fibers": "^4.0.2",
        "jasmine-core": "^3.5.0",
        "jest": "^24.9.0",
        "karma-jasmine": "^2.0.1",
        "sass": "^1.23.3",
        "sass-loader": "^8.0.0",
        "vue-cli-plugin-vuetify": "^2.0.2",
        "vue-jest": "^3.0.5",
        "vue-template-compiler": "^2.6.10"
    },
    "jest": {
        "verbose": true,
        "transform": {
            "^.+\\.(js|jsx|css|less|scss)?$": "<rootDir>/node_modules/babel-jest",
            ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
        },
        "moduleFileExtensions": [
            "js",
            "ts",
            "json",
            "node",
            "vue"
        ],
        "globals": {
            "NODE_ENV": "test"
        },
        "transformIgnorePatterns": [
            "/node_modules/(?!(vuetify)/)"
        ],
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
            "\\.(css|sass)$": "<rootDir>/mocks/styleMock.js"
        }
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "rules": {
            "no-console": "off"
        },
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ]
}
